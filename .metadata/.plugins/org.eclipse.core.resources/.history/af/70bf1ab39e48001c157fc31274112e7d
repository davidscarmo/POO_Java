
public class Conta {


public class Conta {
	double saldo;
	int agencia;
	int numero;
	String titular;

	public void deposita(double valor) {
		this.saldo += valor;
	}

	public boolean saca(double valor) {
		if (this.saldo >= valor) {
			this.saldo -= valor;
			return true;
		} else {
			return false;
		}
	}
	
	public boolean transfere(double valor, Conta destino)
	{
		if(this.saldo >= valor)
		{
			this.saca(300);
			destino.deposita(300);
			return true;
		}
		return false; 
	}
}

